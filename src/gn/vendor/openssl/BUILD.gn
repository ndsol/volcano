source_set("engines") {
  public_configs = [ ":openssl_config" ]
  configs += [ ":openssl_private_config" ]
  sources = [
    "$openssl_root/engines/e_dasync.c",
    "$openssl_root/engines/e_padlock.c",
  ]
  if (is_win) {
    sources += [ "$openssl_root/engines/e_capi.c" ]
  }
  deps = [ ":crypto" ]
}

# :android-crypto is built with a slightly different configs += glfw_config
source_set("android-crypto") {
  public_configs = [ ":openssl_config" ]
  configs += [
    ":openssl_private_config",
    "../glfw:glfw_config",
  ]
  sources = [
    "$buildgn_path/android_bss_file.c",
  ]
}

