declare_args() {
  openssl_root = rebase_path("//vendor/openssl", ".",
                             rebase_path(target_out_dir, root_build_dir))
}

# buildgn_path is the path to this BUILD.gn file
buildgn_path = rebase_path("//src/gn/vendor/openssl", ".",
                           rebase_path(target_out_dir, root_build_dir))

config("openssl_config") {
  include_dirs = [ "$openssl_root/include" ]
  if (is_mac) {
    include_dirs += [ "$buildgn_path/openssl-macos/include" ]
  } else if (is_posix || is_android) {
    include_dirs += [ "$buildgn_path/openssl-android/include" ]
  } else {
    if (defined(buildgn_path)) {
      suppress_warning = buildgn_path
      suppress_warning = []
    }
  }
}

config("openssl_private_config") {
  cflags = []
  include_dirs = []
  if (is_mac) {
    include_dirs += [ "$buildgn_path/openssl-macos" ]
  } else if (is_posix || is_android) {
    include_dirs += [ "$buildgn_path/openssl-android" ]
  }
  include_dirs += [
    "$openssl_root",
    "$openssl_root/crypto/include",
  ]
  defines = [
    "PURIFY",
    "NDEBUG",
    "OPENSSL_THREADS",
    "OPENSSL_PIC",
    "OPENSSL_NO_MD2", "OPENSSL_NO_RC5",
    "OPENSSL_RAND_SEED_OS", "OPENSSL_NO_ASAN", "OPENSSL_NO_ASM",
    "OPENSSL_NO_CRYPTO_MDEBUG", "OPENSSL_NO_CRYPTO_MDEBUG_BACKTRACE",
    "OPENSSL_NO_DEVCRYPTOENG", "OPENSSL_NO_EC_NISTP_64_GCC_128",
    "OPENSSL_NO_EGD", "OPENSSL_NO_EXTERNAL_TESTS", "OPENSSL_NO_FUZZ_AFL",
    "OPENSSL_NO_FUZZ_LIBFUZZER", "OPENSSL_NO_HEARTBEATS", "OPENSSL_NO_MSAN",
    "OPENSSL_NO_SCTP", "OPENSSL_NO_SSL_TRACE", "OPENSSL_NO_SSL3",
    "OPENSSL_NO_SSL3_METHOD", "OPENSSL_NO_UBSAN", "OPENSSL_NO_UNIT_TEST",
    "OPENSSL_NO_WEAK_SSL_CIPHERS", "OPENSSL_NO_DYNAMIC_ENGINE",
    "OPENSSL_NO_AFALGENG"
  ]
  if (is_posix && !is_android) {
    defines += [
      "ENGINESDIR=\"/dev/null\"",
      "OPENSSLDIR=\"/etc/ssl\"",
    ]
  } else if (is_android) {
    defines += [
      "ENGINESDIR=\"lib/engines-1.1\"",
      # android_bss_file.c handles this kind of path
      "OPENSSLDIR=\"assets://ssl\"",
    ]
  }
  if (current_cpu == "x64" || current_cpu == "x86") {
    defines += [
      "DES_PTR",
      "DES_RISC1",
      "DES_UNROLL",
    ]
  }
  if (!is_win) {
    if (is_clang) {
      cflags += [
        "-Wno-parentheses-equality",
        "-Wno-unused-value",
      ]
    } else {
      cflags += [
        "-Wno-unused-variable",
        "-Wno-unused-parameter",
        "-Wno-strict-overflow",
        "-Wno-missing-field-initializers",
      ]
    }
  }
}

